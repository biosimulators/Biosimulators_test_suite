            "field": "biggId"
          },
          "align": {
            "value": "left"
          }
        },
        "update": {
          "x": {
            "field": "labelXScaled"
          },
          "y": {
            "field": "labelYScaled"
          },
          "fontSize": {
            "field": "fontSizeScaled"
          },
          "fill": {
            "field": "fill"
          }
        }
      }
    },
    {
      "name": "Reaction labels",
      "type": "text",
      "from": {
        "data": "reactionLabelsVisibleScaled"
      },
      "encode": {
        "enter": {
          "text": {
            "field": "biggId"
          },
          "align": {
            "value": "left"
          }
        },
        "update": {
          "x": {
            "field": "xScaled"
          },
          "y": {
            "field": "yScaled"
          },
          "fontSize": {
            "field": "fontSizeScaled"
          },
          "fill": {
            "field": "fill"
          }
        }
      }
    }
  ],
  "legends": [{
    "fill": "reactionFluxColorScale",
    "type": "gradient",
    "title": "Fluxes",
    "titleFontSize": 10,
    "titlePadding": 2,
    "titleAlign": "left",
    "orient": "top-right",
    "direction": "vertical"
  }]
}
