          "shape": {
            "value": "circle"
          },
          "tooltip": {
            "field": "idName"
          }
        },
        "update": {
          "xc": {
            "field": "xScaled"
          },
          "yc": {
            "field": "yScaled"
          },
          "size": {
            "field": "sizeScaled"
          },
          "fill": {
            "field": "fill"
          },
          "stroke": {
            "field": "stroke"
          },
          "strokeWidth": {
            "field": "strokeWidthScaled"
          }
        }
      }
    },
    {
